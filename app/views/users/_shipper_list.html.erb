<h2 class = "title">Shippers</h2>

<% unless @shippers.nil? || @shippers.empty? %>
  <table class="table table-striped">
    <thead>
      <tr>
        <th width="250">Name</th>
        <th width="125">Email</th>

      </tr>
    </thead>
    <tbody>
      <% @shippers.each do |c| %>
        <tr>
          <td><%= link_to c.proper_name, user_path(c) %> </td>
          <td><%= c.email %></td>

        </tr>
      <% end %>
    </tbody>
  </table>
<% else %>
  <p>There are no items that need to be reorder at this time.</p>
<% end %>
